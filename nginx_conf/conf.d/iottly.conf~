upstream node-app {
              least_conn;
              server deviottlycore.iottlydocker:8520 weight=10 max_fails=3 fail_timeout=30s;
}




server {
    listen       8550;
    server_name  localhost;

    location / {
       proxy_pass http://node-app;
    }
    location /static {
       root /iottly_console;
    }
    error_page   500 502 503 504  /50x.html;
    location = /50x.html {
        root   /usr/share/nginx/html;
    }

}
